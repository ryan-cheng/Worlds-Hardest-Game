import zen.core.Zen;

public class playerr {
	int x = 250;
	int y = 250;

	public playerr(int x, int y) {
		x = this.x;
		y = this.y;
	}

	public void draw() {
		Zen.setColor("orange");
		Zen.fillRect(x, y, 10, 10);
	}

	public void move() {

		if (Zen.isKeyPressed("right")) {
			x = x + 5;
		}
		if (Zen.isKeyPressed("left")) {
			x = x - 5;
		}
		if (Zen.isKeyPressed("up")) {
			y = y - 5;
		}
		if (Zen.isKeyPressed("down")) {
			y = y + 5;
		}

	}

}
